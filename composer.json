{
    "name": "biteit/vokativ",
    "description": "Knihovna pro skloňování českých jmen do 5. pádu pro účely generování oslovení.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Petr Joachim",
            "email": "petr@joachim.cz"
        },
        {
            "name": "Radek Boszczyk",
            "email": "radek@biteit.cz"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Vokativ\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vokativ\\Test\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php test"
    }
}
